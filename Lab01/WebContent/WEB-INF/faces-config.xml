<?xml version="1.0" encoding="UTF-8"?>

<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd"
	version="1.2">
	<managed-bean>
		<managed-bean-name>hello</managed-bean-name>
		<managed-bean-class>HelloBean</managed-bean-class>
		<managed-bean-scope>application</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>timeline</managed-bean-name>
		<managed-bean-class>Timeline</managed-bean-class>
		<managed-bean-scope>application</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>linkCreator</managed-bean-name>
		<managed-bean-class>LinkCreator</managed-bean-class>
		<managed-bean-scope>application</managed-bean-scope>
	</managed-bean>

</faces-config>
